{
  "compilerOptions": {
    "outDir": "./dist/",
    "paths": {
      "@/*": ["./src/*"]
    },
	"types": ["jest"],
    "lib": ["ESNext", "DOM"],
    "module": "ESNext",
    "target": "ES6",
    "moduleResolution": "Node",
    "strict": true,
    "strictNullChecks": true,
    "sourceMap": true,
    "noImplicitAny": true,
	"esModuleInterop": true
  }
}
